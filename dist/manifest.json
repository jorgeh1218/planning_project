{
    "_version": "1.1.0",
    "sap.app": {
        "_version": "1.1.0",
        "id": "PlanificadorAppv2",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "sourceTemplate": {
            "id": "ui5template.basicSAPUI5ApplicationProject",
            "version": "1.32.0"
        }
    },
    "sap.ui": {
        "_version": "1.1.0",
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        },
        "supportedThemes": [
            "sap_hcb",
            "sap_bluecrystal"
        ]
    },
    "sap.ui5": {
        "_version": "1.1.0",
        "rootView": {
            "viewName": "PlanificadorAppv2.view.App",
            "type": "XML"
        },
        "dependencies": {
            "minUI5Version": "1.30.0",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {},
                "sap.ui.layout": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "tiles": {
                "type": "sap.ui.model.json.JSONModel",
                "uri": "model/tiles.json"
            },
            "data": {
                "type": "sap.ui.model.json.JSONModel",
                "uri": "model/data.json"
            },
            "settings": {
                "type": "sap.ui.model.json.JSONModel",
                "uri": "model/settings.json"
            },
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "PlanificadorAppv2.i18n.i18n"
                }
            },
            "@i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "PlanificadorAppv2.i18n.i18n"
                }
            }
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "viewPath": "PlanificadorAppv2.view",
                "controlId": "appControl",
                "controlAggregation": "pages",
                "transition": "slide"
            },
            "routes": [
                {
                    "pattern": "",
                    "name": "initial",
                    "target": "initial"
                },
                {
                    "pattern": "main",
                    "name": "main",
                    "target": "main"
                },
                {
                    "pattern": "editPlan",
                    "name": "form",
                    "target": "form"
                },
                {
                    "pattern": "editPlanSupplies",
                    "name": "formSupplies",
                    "target": "formSupplies"
                },
                {
                    "pattern": "editPlanS",
                    "name": "formSanitary",
                    "target": "formSanitary"
                }
            ],
            "targets": {
                "initial": {
                    "viewName": "initial",
                    "viewLevel": 1
                },
                "main": {
                    "viewName": "main",
                    "viewLevel": 2
                },
                "form": {
                    "viewName": "form",
                    "viewLevel": 3
                },
                "formSupplies": {
                    "viewName": "formSupplies",
                    "viewLevel": 3
                },
                "formSanitary": {
                    "viewName": "formSanitary",
                    "viewLevel": 3
                }
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        }
    },
    "sap.platform.hcp": {
        "uri": "",
        "_version": "1.1.0"
    }
}